	lucy axolotl:
		name: axolotl¦s
		patterns:
			0: <age> lucy axolotl[plural:s]
			1: baby:lucy axolotl (kid[plural:s]|child[plural:ren])
	wild axolotl:
		name: axolotl¦s
		patterns:
			0: <age> wild axolotl[plural:s]
